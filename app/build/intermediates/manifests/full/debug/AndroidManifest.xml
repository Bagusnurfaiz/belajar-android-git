<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="yoga.fajar.simpleapplication"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="26" />

    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:name="yoga.fajar.simpleapplication.AppApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="yoga.fajar.simpleapplication.Splashscreen"
            android:noHistory="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="yoga.fajar.simpleapplication.LoginActivity" />
        <activity android:name="yoga.fajar.simpleapplication.MainActivity" />
        <activity android:name="yoga.fajar.simpleapplication.pindahActivity" />
        <activity android:name="yoga.fajar.simpleapplication.FragmentActivity" />
        <activity android:name="yoga.fajar.simpleapplication.activity.AddUserActivity" />
        <activity android:name="yoga.fajar.simpleapplication.activity.DetailActivity" />

        <meta-data
            android:name="android.support.VERSION"
            android:value="26.1.0" />
        <meta-data
            android:name="android.arch.lifecycle.VERSION"
            android:value="27.0.0-SNAPSHOT" />
    </application>

</manifest>